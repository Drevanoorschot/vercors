
predicate valid(b: Bool) {
    b ? true : false
}

method t1(this: Ref, b: Bool)
    requires acc(false.valid(), write)
{
    assert false
}

method t2(this: Ref, b: Bool)
    requires acc(false.valid(), write)
{
    unfold acc(false.valid(), write)
    assert false
}

method t3(this: Ref, b: Bool)
    requires acc(true.valid(), write)
{
    unfold acc(true.valid(), write)
    //:: ExpectedError(assert.failed:assertion.false)
    assert false
}
