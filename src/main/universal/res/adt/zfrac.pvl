adt zfrac {
  pure rational zfrac_val(f: zfrac);

  axiom
    (\forall zfrac f; true;
      (\forall zfrac g; true;
        {:zfrac_val(f):} == {:zfrac_val(g):} ==> f == g));

  axiom
    (\forall zfrac f; true; none <= {:zfrac_val(f):} <= write);
}

requires none <= perm <= write;
ensures zfrac_val(\result) == perm;
pure zfrac new_zfrac(perm: rational);