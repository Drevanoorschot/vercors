method m1(b1: Bool, b2: Bool)
{
  var a: Int
  if(b1 ==> b2) {
    a := 1
  }
  else {
    a := 2
  }
}

method m2(b1: Bool, b2: Bool)
{
  var a: Int
  if(false ==> b2) {
    a := 1
  }
  else {
    a := 2
  }
}

method m3(b1: Bool, b2: Bool)
{
  var a: Int
  if(b1 ==> true) {
    a := 1
  }
  else {
    a := 2
  }
}