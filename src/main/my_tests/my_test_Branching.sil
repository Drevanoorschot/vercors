method m1(b: Bool) returns (a: Int)
{
  a := 1
  if(a == 1){
    a := (b ? ((a==1) ? 2 : 3) : ((a!=1) ? 4 : 5))
  }
  else{
    a := -2
  }


  if(!!b){
    a := 2
  }
  else{
    a := 3
  }
  a := 4
  //assert(1==2)
}

method m2(b: Bool, c: Bool)
{
  var a: Int
  if(b) {
    a := (c ? 1 : 2)
  }
  else {
    a := 3 + 4
  }
}