class SimpleCollectionConstructors {
	void main() {
		seq<int> a = seq<int> {};
		seq<int> b = seq<int> {1, 5, 7, 9, 2};

		seq<int> c = [t:int];
		seq<int> d = [1, 5, 7, 9, 2];

		assert a == c && b == d;
		
		Edge e1 = new Edge(0, 1);
		Edge e2 = new Edge(1, 2);
		seq<Edge> es = [t:Edge];

		assert [e1, e2] == e1::e2::[t:Edge];
	}
}

class Edge {
	int source;
	int target;

	Edge(int s, int t) {
		source = s;
		target = t;
	}
}
